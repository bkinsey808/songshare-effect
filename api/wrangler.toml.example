name = "songshare-effect-api"
main = "src/index.ts"
compatibility_date = "2024-09-23"

[dev]
port = 8787

[vars]
ENVIRONMENT = "development"
VITE_SUPABASE_URL = "https://[something].supabase.co"
SUPABASE_SERVICE_KEY = "[your_service_key]"
SUPABASE_VISITOR_EMAIL = "[your_visitor_email]"
SUPABASE_VISITOR_PASSWORD = "[your_visitor_password]"

[[r2_buckets]]
binding = "BUCKET"
bucket_name = "[your_bucket_name]"
preview_bucket_name = "[your_preview_bucket_name]"

[env.production.vars]
ENVIRONMENT = "production"