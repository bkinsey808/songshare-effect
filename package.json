{
	"name": "songshare-effect",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:api": "cd api && pnpm dev",
		"dev:all": "npx concurrently \"pnpm dev\" \"pnpm dev:api\" --names \"frontend,api\" --prefix-colors \"blue,green\"",
