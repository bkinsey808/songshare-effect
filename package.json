		"globals": "^16.4.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"react-compiler-healthcheck": "19.1.0-rc.3",
		"tailwindcss": "^4.1.13",
		"typescript": "~5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "npm:rolldown-vite@7.1.12"
	},
	"overrides": {
